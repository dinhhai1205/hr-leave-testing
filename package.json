  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "pm2 start .build/main.js",
    "start:dev": "NODE_ENV=local node .build/main.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "TZ=UTC node .build/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "node ./scripts/migration-create.script.mjs",
    "gen:key-pair": "node ./scripts/generate-key-pair.mjs",
    "start:db-init": "cd deployment/local/docker && docker-compose up -d",
    "check:no-cycle": "npx madge --circular src/main.ts"
  },
